<window id="8011">
	<defaultcontrol always="true">59</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<zorder>999</zorder>
	<controls>
		<control type="group">
			<posx>180</posx>
			<posy>45</posy>
			<width>960</width>
			<height>600</height>
			<!-- <visible>!Window.IsVisible(FileBrowser)</visible>-->
			<animation easing="out" effect="slide" end="0,0" start="1100,0" time="400" tween="quadratic">WindowOpen</animation>
			<animation easing="out" effect="slide" end="1100,0" start="0,0" time="400" tween="quadratic">WindowClose</animation>
			<control type="image">
				<posx>0</posx>
				<posy>0</posy>
				<width>960</width>
				<height>600</height>
				<texture border="15,0,0,0" flipx="true">MediaBladeSub.png</texture>
			</control>
			<control type="button">
				<description>Close Window button</description>
				<posx>30</posx>
				<posy>0</posy>
				<width>64</width>
				<height>32</height>
				<label>-</label>
				<font>-</font>
				<onclick>PreviousMenu</onclick>
				<texturefocus>DialogCloseButton-focus.png</texturefocus>
				<texturenofocus>DialogCloseButton.png</texturenofocus>
				<onleft>9000</onleft>
				<onright>9000</onright>
				<onup>9000</onup>
				<ondown>9000</ondown>
				<visible>system.getbool(input.enablemouse)</visible>
			</control>
			<control type="label">
				<posx>0</posx>
				<posy>0</posy>
				<width>960</width>
				<height>20</height>
				<textcolor>white</textcolor>
				<selectedcolor>white</selectedcolor>				
				<info>Choose correct title for this movie</info>
			</control>
			<control id="59" type="list">
				<description>List of movies</description>
				<posx>20</posx>
				<posy>40</posy>
				<width>920</width>
				<height>600</height>
				<onleft>59</onleft>
				<onright>59</onright>
				<onup>9000</onup>
				<ondown>59</ondown>
				<scrolltime>200</scrolltime>
				<orientation>vertical</orientation>
				<!-- <viewtype label="535">list</viewtype> -->
				<pagecontrol>-</pagecontrol>
				<itemlayout height="140" width="920">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>100</width>
						<height>140</height>
						<info>ListItem.Icon</info>
						<bordersize>5</bordersize>
						<texture background="true">$INFO[ListItem.Icon]</texture>
					</control>
					<control type="label">
						<posx>140</posx>
						<posy>0</posy>
						<width>700</width>
						<height>140</height>
						<info>ListItem.Label</info>
						<font>font13</font>
						<align>left</align>
						<aligny>center</aligny>
						<textcolor>white</textcolor>
						<selectedcolor>white</selectedcolor>
					</control>
				</itemlayout>
				<focusedlayout height="140" width="920">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>100</width>
						<height>140</height>
						<info>ListItem.Icon</info>
						<bordersize>5</bordersize>
						<texture background="true">$INFO[ListItem.Icon]</texture>
					</control>
					<control type="label">
						<posx>140</posx>
						<posy>0</posy>
						<width>700</width>
						<height>140</height>
						<info>ListItem.Label</info>
						<font>font13</font>
						<align>left</align>
						<aligny>center</aligny>
						<textcolor>white</textcolor>
						<selectedcolor>white</selectedcolor>
					</control>
				</focusedlayout>
			</control>
		</control>
	</controls>
</window>