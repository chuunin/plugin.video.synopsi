			<font>font12_title</font>
			<textcolor>white</textcolor>
			<focusedcolor>white</focusedcolor>
			<align>center</align>
			<onleft>10</onleft>
			<onright>11</onright>
			<onup>15</onup>
			<ondown>15</ondown>
		</control>
