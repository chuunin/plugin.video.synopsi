			<font>font12_title</font>
			<textcolor>white</textcolor>
			<focusedcolor>white</focusedcolor>
			<align>center</align>
			<onleft>10</onleft>
			<onright>16</onright>
			<onup>10</onup>
			<ondown>16</ondown>
		</control>
