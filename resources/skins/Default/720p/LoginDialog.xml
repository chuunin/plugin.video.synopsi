<window id="5000">
	<defaultcontrol always="true">10</defaultcontrol>
	<coordinates>
		<system>1</system>
		<posx>335</posx>
		<posy>230</posy>
	</coordinates>
	<include>dialogeffect</include>
	<controls>
		<control type="image">
			<posx>0</posx>
			<posy>0</posy>
			<width>610</width>
			<height>340</height>
			<texture border="40">DialogBack.png</texture>
		</control>
		<control type="image">
			<description>Dialog Header image</description>
			<posx>40</posx>
			<posy>16</posy>
			<width>530</width>
			<height>40</height>
			<texture>dialogheader.png</texture>
		</control>
		<control id="1" type="label">
			<description>header label</description>
			<posx>40</posx>
			<posy>20</posy>
			<width>530</width>
			<height>30</height>
			<font>font13_title</font>
			<label>Log In</label>
			<!-- <label>$LOCALIZE[SCRIPT69604]</label> -->
			<!-- <label>$LOCALIZE[69604]</label> -->
			<align>center</align>
			<aligny>center</aligny>
			<textcolor>selected</textcolor>
			<shadowcolor>black</shadowcolor>
		</control>
		<control type="button">
			<description>Close Window button</description>
			<posx>520</posx>
			<posy>15</posy>
			<width>64</width>
			<height>32</height>
			<label>-</label>
			<font>-</font>
			<onclick>PreviousMenu</onclick>
			<texturefocus>DialogCloseButton-focus.png</texturefocus>
			<texturenofocus>DialogCloseButton.png</texturenofocus>
			<onleft>10</onleft>
			<onright>10</onright>
			<onup>10</onup>
			<ondown>10</ondown>
			<visible>system.getbool(input.enablemouse)</visible>
		</control>
		<control id="2" type="label">
			<description>dialog line 1</description>
			<posx>30</posx>
			<posy>60</posy>
			<width>550</width>
			<height>60</height>
			<wrapmultiline>true</wrapmultiline>
			<align>center</align>
			<aligny>center</aligny>
			<label>Your login and passphrase didn't match. Please reenter your login info.</label>
			<font>font13</font>
		</control>
		<control id="6" type="label">
			<description>Login label</description>
			<posx>35</posx>
			<posy>135</posy>
			<width>165</width>
			<height>40</height>
			<align>center</align>
			<aligny>center</aligny>
			<label>Login</label>
			<font>font13</font>
		</control>		
		<control id="10" type="edit">
			<description>Login</description>
			<posx>205</posx>
			<posy>135</posy>
			<width>350</width>
			<height>40</height>
			<font>font12_title</font>
			<textcolor>white</textcolor>
			<focusedcolor>white</focusedcolor>
			<onleft>11</onleft>
			<onright>11</onright>
			<onup>11</onup>
			<ondown>11</ondown>
			<visible>true</visible>
		</control>
		<control id="7" type="label">
			<description>Passphrase label</description>
			<posx>35</posx>
			<posy>185</posy>
			<width>165</width>
			<height>40</height>
			<align>center</align>
			<aligny>center</aligny>
			<label>Passphrase</label>
			<font>font13</font>
		</control>				
		<control id="11" type="edit">
			<description>Passphrase</description>
			<password>true</password>
			<posx>205</posx>
			<posy>185</posy>
			<width>350</width>
			<height>40</height>
			<font>font12_title</font>
			<textcolor>white</textcolor>
			<focusedcolor>white</focusedcolor>
			<visible>true</visible>
			<onleft>10</onleft>
			<onright>15</onright>
			<onup>10</onup>
			<ondown>15</ondown>
		</control>
		<control id="15" type="button">
			<description>Ok</description>
			<posx>30</posx>
			<posy>250</posy>
			<width>200</width>
			<height>40</height>
