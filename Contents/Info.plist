<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
     <key>PlexPluginClass</key>
     <string>Agent</string>
     <key>CFBundleIdentifier</key>
     <string>com.plexapp.plugins.synopsi</string>
     <key>PlexAudioCodec</key>
     <array>
             <string>AAC</string>
     </array>
     <key>PlexClientPlatforms</key>
     <string>MacOSX,iOS,Android,LGTV</string>
     <key>PlexFrameworkVersion</key>
     <string>2</string>
     <key>PlexMediaContainer</key>
     <array>
             <string>M4V</string>
             <string>MOV</string>
     </array>
     <key>PlexVideoCodec</key>
     <array>
             <string>H.264</string>
     </array>
     <key>PlexURLServices</key>
     <dict>
             <key>Apple Movie Trailers</key>
             <dict>
                     <key>URLPattern</key>
                <string>http://.*apple\.com/.*</string>
                <key>Identifier</key>
                <string>com.plexapp.plugins.amt</string>
             </dict>
     </dict>
</dict>
</plist>
