<?xml version="1.0" encoding="UTF-8"?>
<scraper framework="1.1" date="2013-03-01">
	<CreateSearchUrl dest="3">
		<RegExp input="$$1" output="&lt;url&gt;http://localhost:9091/search/?q=\1&lt;/url&gt;" dest="3">
			<expression noclean="1"></expression>
		</RegExp>
	</CreateSearchUrl>
	<NfoUrl dest="2">
	</NfoUrl>
	<GetSearchResults dest="4">
		<RegExp input="$$3" output="&lt;?xml version=&quot;1.0&quot; encoding=&quot;utf-8&quot; standalone=&quot;yes&quot;?&gt;&lt;results&gt;&lt;entity&gt;&lt;title&gt;Fake Title&lt;/title&gt;&lt;url&gt;http://localhost:9091/get_detail/&lt;/url&gt;&lt;/entity&gt;&lt;/results&gt;" dest="4">
			<expression noclean="1"></expression>
		</RegExp>
	</GetSearchResults>
	<GetDetails dest="6">
		<RegExp input="$$1" output="&lt;?xml version=&quot;1.0&quot; encoding=&quot;utf-8&quot; standalone=&quot;yes&quot;?&gt;&lt;details&gt;
   &lt;title&gt;N/A&lt;/title&gt;
   &lt;year&gt;&lt;/year&gt;
   &lt;director&gt;&lt;/director&gt;
   &lt;top250&gt;&lt;/top250&gt;
   &lt;mpaa&gt;&lt;/mpaa&gt;
   &lt;tagline&gt;&lt;/tagline&gt;
   &lt;runtime&gt;&lt;/runtime&gt;
   &lt;thumb&gt;&lt;/thumb&gt;
   &lt;credits&gt;&lt;/credits&gt;
   &lt;rating&gt;&lt;/rating&gt;
   &lt;votes&gt;&lt;/votes&gt;
   &lt;genre&gt;&lt;/genre&gt;
   &lt;outline&gt;&lt;/outline&gt;
   &lt;plot&gt;&lt;/plot&gt;
 &lt;/details&gt;" dest="6">
			<expression noclean="1"></expression>
		</RegExp>
	</GetDetails>

