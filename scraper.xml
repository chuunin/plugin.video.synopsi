<?xml version="1.0" encoding="UTF-8"?>
<scraper framework="1.1" date="2013-03-01">
	<CreateSearchUrl dest="3">
		<RegExp input="$$1" output="&lt;url&gt;http://localhost:9099/search/?q=\1&lt;/url&gt;" dest="3">
			<expression noclean="1"></expression>
		</RegExp>
	</CreateSearchUrl>
	<NfoUrl dest="2">
	</NfoUrl>
	<GetSearchResults dest="4">
		<RegExp input="$$5" output="&lt;?xml version=&quot;1.0&quot; encoding=&quot;utf-8&quot; standalone=&quot;yes&quot;?&gt;&lt;results&gt;\1&lt;/results&gt;" dest="4">
			<RegExp input="$$1" output="&lt;entity&gt;&lt;title&gt;\1&lt;/title&gt;&lt;url&gt;http://localhost:9099/get_detail/?q=\1&lt;/url&gt;&lt;/entity&gt;" dest="5">
				<expression>&lt;id&gt;(.*)&lt;/id&gt;</expression>
			</RegExp>
			<expression noclean="1"></expression>
		</RegExp>
	</GetSearchResults>
	<GetDetails dest="6">
		<RegExp input="$$1" output="&lt;?xml version=&quot;1.0&quot; encoding=&quot;utf-8&quot; standalone=&quot;yes&quot;?&gt;&lt;details&gt;\1&lt;/details&gt;" dest="6">
			<expression noclean="1"></expression>
		</RegExp>
	</GetDetails>

